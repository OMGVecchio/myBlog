{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "plugins": [
    "react",
    "jsx-a11y",
    "import"
  ],
  "rules": {
    "strict": 0,
    "semi": 0,
    "comma-dangle": [2, "never"],
    "import/no-unresolved": [2, {"ignore": ["components", "static", "store", "utils"]}],
    "import/extensions": 0,
    "react-in-jsx-scope": 0,
    "import/no-extraneous-dependencies": 0,
    "react/prop-types": 0,
    "react/react-in-jsx-scope": 0,
    "react/jsx-filename-extension": 0,
    "react/no-danger": 0,
    "jsx-a11y/click-events-have-key-events": 0,
    "jsx-a11y/anchor-is-valid": 0
  },
  "env": {
    "browser": true
  },
  "globals": {
    "__NEXT_DATA__": true,
    "adblockWorkWell": true
  }
}
