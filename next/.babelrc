{
    "presets": [
        "next/babel"
    ],
    "plugins": [
        [
            "styled-jsx/babel",
            {
                "plugins": [
                    "styled-jsx-plugin-less"
                ]
            }
        ], [
            "module-resolver",
            {
                "cwd": "babelrc",
                "root": ["."],
                "alias": {
                    "components": "./components",
                    "static": "./static"
                }
            }
        ]
    ]
}
